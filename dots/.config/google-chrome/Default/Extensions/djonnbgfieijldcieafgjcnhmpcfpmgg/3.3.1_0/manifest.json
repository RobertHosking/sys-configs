{
   "app": {
      "background": {
         "scripts": [ "background.js" ]
      }
   },
   "description": "The most powerful photo editor on Chrome. Only 3MB and works offline.",
   "icons": {
      "128": "128.png",
      "512": "512.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "36.0",
   "name": "Polarr Photo Editor 3",
   "offline_enabled": true,
   "permissions": [ "https://v3.polarr.co/", "https://photoeditor.polarr.co/", "https://www.polarr.co/", "https://www.google-analytics.com/", "fileSystem", {
      "fileSystem": [ "write", "retainEntries", "directory" ]
   }, "notifications", "unlimitedStorage", "storage", "system.display", "system.memory", "system.storage", "system.cpu", "app.window.fullscreen" ],
   "requirements": {
      "3D": {
         "features": [ "webgl" ]
      }
   },
   "short_name": "Polarr",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.3.1"
}
